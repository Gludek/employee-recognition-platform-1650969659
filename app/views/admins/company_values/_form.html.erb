<%= form_with(model: [:admins,company_value]) do |f| %>
  <% if company_value.errors.any? %>
    <div id="error_explanation notification is-warning">
      <h2><%= pluralize(company_value.errors.count, "error") %> prohibited this company value from being saved:</h2>
      <ul>
        <% company_value.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div class="field">
    <%= f.label :title, class:'label' %>
    <div class='control'>
     <%= f.text_field :title,value: company_value.title, autofocus: true,class:'input' %>
    </div>
  </div>
    
  <div class="actions container">
    <%= f.submit  class:'button is-primary' %>
  </div>
<% end %>
